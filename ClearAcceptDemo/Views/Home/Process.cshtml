@model ProcessViewModel
@{
ViewData["Title"] = "ClearAccept Demo";
ViewData["Subtitle"] = "Confirm PaymentRequest";
}

<div class="row mt-3">
    <a href="~/"><i class="fa fa-backward font-primary"></i></a>
</div>

<hr />
<h8>@Model.Result</h8>
<hr />

<div class="container-fluid pt-5">
    <div class="row">
        <div class="col">
            <textarea class="process-result" spellcheck="false">Request:&#13;&#13;@Model.Request</textarea>
        </div>
        <div class="col">
            <textarea class="process-result" spellcheck="false">Response:&#13;&#13;@Model.Response</textarea>
        </div>
    </div>
</div>